  "scripts": {
    "prepare": "husky install",
    "build": "FEDERATE=1 webpack --mode production --config webpack.prod.js",
    "start": "DEMO_APP=true webpack serve --https --host prod.foo.redhat.com --port 1337 --hot --config webpack.dev.js",
    "start:mocked": "DEMO_APP=true MOCKED_API=true INSPECT=true webpack serve --hot --config webpack.dev.js",
    "start:mocked:it": "DEMO_APP=true MOCKED_API=true webpack serve --hot --config webpack.dev.js --no-open",
    "start:federate": "webpack serve --host localhost --port 9006 --config webpack.dev.js",
    "test": "jest --verbose",
    "test:watch": "jest --watch --verbose",
    "cypress:open:it": "cypress open --project cypress --config-file cypress.config.it.ts",
    "cypress:run:it": "cypress run --browser chrome --project cypress --config-file cypress.config.it.ts",
    "cypress:open:e2e": "cypress open --browser chrome --project cypress --config-file cypress.config.e2e.ts",
    "cypress:run:e2e": "cypress run --browser chrome --project cypress --config-file cypress.config.e2e.ts",
    "test:it": "start-server-and-test 'start:mocked:it' http://localhost:8080 'cypress:run:it'",
    "test:e2e": "start-server-and-test 'start' https://prod.foo.redhat.com:1337 'cypress:run:e2e'",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
